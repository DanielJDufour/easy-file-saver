{
  "name": "easy-file-saver",
  "version": "0.0.0",
  "description": "Very Easily Save a File",
  "main": "easy-file-saver.js",
  "files": [
    "easy-file-saver.js"
  ],
  "scripts": {
    "dev": "concurrently \"npm run serve\" \"npm test\"",
    "format": "npx prettier --arrow-parens=avoid --print-width=160 --trailing-comma=none --write *.js",
    "serve": "npx live-server --ignore=node_modules",
    "test": "ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielJDufour/easy-file-saver.git"
  },
  "keywords": [
    "arraybuffer",
    "buffer",
    "blob",
    "csv",
    "file",
    "jpg",
    "json",
    "png",
    "save",
    "tsv",
    "txt",
    "webp"
  ],
  "author": "Daniel J. Dufour",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/DanielJDufour/easy-file-saver/issues"
  },
  "homepage": "https://github.com/DanielJDufour/easy-file-saver#readme",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "live-server": "^1.2.1"
  }
}
